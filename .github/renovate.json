{
  "extends": [
    "config:base",
    "schedule:monthly"
  ],
  "baseBranches": [
    "develop"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ],
  "automergeType": "branch",
  "dependencyDashboard": false
}
